<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<EditBox name="IN_EditBoxTemplate" font="GameFontNormal" autoFocus="false" virtual="true">
		<Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<EdgeSize><AbsValue val="16"/></EdgeSize>
			<TileSize><AbsValue val="32"/></TileSize>
			<BackgroundInsets><AbsInset left="5" right="5" top="5" bottom="5"/></BackgroundInsets>
			<TextInsets><AbsInset left="8" right="5" top="5" bottom="5"/></TextInsets>
		</Backdrop>
	</EditBox>

	<Frame name="IN_Frame" toplevel="true" parent="UIParent" frameStrata="DIALOG" hidden="true" enableMouse="true" moveable="true">
		<Size>
			<AbsDimension x="380" y="190"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<TitleRegion setAllPoints="true"/>
		<Scripts>
		</Scripts>
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\TutorialFrame\TutorialFrameBorder" tile="true">
			<BackgroundInsets>
				<AbsInset left="4" right="4" top="4" bottom="4"/>
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32"/>
			</TileSize>
			<EdgeSize>
				<AbsValue val="32"/>
			</EdgeSize>
		</Backdrop>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="$parent_Title" inherits="GameFontNormal" text="Item Note">
					<Anchors>
						<Anchor point="TOP" relativeTo="IN_Frame">
							<Offset>
								<AbsDimension x="-10" y="-6"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="TEXT">
			</Layer>			
		</Layers>
		<Frames>
			<Button name="$parent_btnX" inherits="OptionsButtonTemplate" text="X">
				<Size><AbsDimension x="25" y="25"/></Size>
				<Anchors><Anchor point="TOPRIGHT"/></Anchors>
				<Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
					<EdgeSize><AbsValue val="16"/></EdgeSize>
					<TileSize><AbsValue val="32"/></TileSize>
					<BackgroundInsets><AbsInset left="5" right="5" top="5" bottom="5"/></BackgroundInsets>
				</Backdrop>
				<Scripts><OnClick> IN_ButtonClick(self) </OnClick></Scripts>
			</Button>
			<ScrollFrame name="$parent_sfContent" inherits="UIPanelScrollFrameTemplate">
				<Size><AbsDimension x="350" y="120" /></Size>
				<Anchors><Anchor point="TOPLEFT"><Offset><AbsDimension x="1" y="-25" /></Offset></Anchor></Anchors>
				<Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
					<EdgeSize><AbsValue val="16"/></EdgeSize>
					<TileSize><AbsValue val="32"/></TileSize>
					<BackgroundInsets><AbsInset left="5" right="5" top="5" bottom="5"/></BackgroundInsets>
				</Backdrop>
				<ScrollChild>
					<EditBox name="IN_Frame_ebxContent" multiLine="true" autoFocus="false">
						<Size x="340" y="110" />
						<Anchors><Anchor point="TOPLEFT"><Offset><AbsDimension x="5" y="-5" /></Offset></Anchor></Anchors>
						<FontString inherits="GameFontHighlight"/>
						<Scripts>
							<OnEscapePressed> IN_escapePressed() </OnEscapePressed>
						</Scripts>
					</EditBox>
				</ScrollChild>
				<Scripts>
					<OnMouseUp> IN_scrollFrameClick() </OnMouseUp>
				</Scripts>
			</ScrollFrame>
			<Button name="$parent_btnCancel" inherits="OptionsButtonTemplate" text="Cancel">
				<Size><AbsDimension x="70" y="30"/></Size>
				<Anchors><Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-5" y="10"/></Offset></Anchor></Anchors>
				<Scripts>
					<OnClick> IN_ButtonClick(self) </OnClick>
				</Scripts>
			</Button>
			<Button name="$parent_btnOK" inherits="OptionsButtonTemplate" text="OK">
				<Size><AbsDimension x="70" y="30"/></Size>
				<Anchors><Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-80" y="10"/></Offset></Anchor></Anchors>
				<Scripts>
					<OnClick> IN_ButtonClick(self) </OnClick>
				</Scripts>
			</Button>
			<Button name="$parent_btnImport" inherits="OptionsButtonTemplate" text="Import">
				<Size><AbsDimension x="70" y="30"/></Size>
				<Anchors><Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-155" y="10"/></Offset></Anchor></Anchors>
				<Scripts>
					<OnClick> IN_ButtonClick(self) </OnClick>
				</Scripts>
			</Button>
			<Button name="$parent_btnExport" inherits="OptionsButtonTemplate" text="Export">
				<Size><AbsDimension x="70" y="30"/></Size>
				<Anchors><Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-80" y="10"/></Offset></Anchor></Anchors>
				<Scripts>
					<OnClick> IN_ButtonClick(self) </OnClick>
				</Scripts>
			</Button>
		</Frames>
	</Frame>
	<Script file="ItemNotes.lua" />
</Ui>